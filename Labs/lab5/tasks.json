{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build lab5 (intlist2)",
      "type": "shell",
      "command": "/bin/zsh",
      "args": [
        "-lc",
        "mkdir -p build && clang -std=c11 -Wall -Wextra -g main.c intlist_common.c intlist2.c -o build/main"
      ],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "run lab5 (intlist2)",
      "type": "shell",
      "command": "/bin/zsh",
      "args": [
        "-lc",
        "./build/main"
      ],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "dependsOn": "build lab5 (intlist)",
      "problemMatcher": []
    }
  ]
}
